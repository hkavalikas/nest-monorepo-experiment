{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@client/(.*)$": "<rootDir>/../../client/src/$1",
    "^@common/(.*)$": "<rootDir>/../../common/$1"
  }
}