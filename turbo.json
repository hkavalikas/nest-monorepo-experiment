{
  "extends": [
    "//"
  ],
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": []
    },
    "dev:run": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:app": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "src/**/*.ts",
        "!src/tests/**/*.ts",
        "eslint.config.mjs"
      ],
      "outputs": []
    },
    "lint:run": {
      "cache": true,
      "inputs": [
        "src/**/*.ts",
        "!src/tests/**/*.ts",
        "eslint.config.mjs"
      ],
      "outputs": []
    },
    "format": {
      "cache": true,
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "format:run": {
      "cache": true,
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**/*.ts",
        "jest.config.js"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:unit": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**/*.ts",
        "src/tests/unit/**/*.ts"
      ],
      "outputs": []
    },
    "test:unit:run": {
      "cache": true,
      "inputs": [
        "src/**/*.ts",
        "src/tests/unit/**/*.ts"
      ],
      "outputs": []
    },
    "test:e2e": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**/*.ts",
        "src/tests/jest-e2e.json",
        "src/tests/e2e/**/*.ts"
      ],
      "outputs": []
    },
    "test:e2e:run": {
      "cache": true,
      "inputs": [
        "src/**/*.ts",
        "src/tests/jest-e2e.json",
        "src/tests/e2e/**/*.ts"
      ],
      "outputs": []
    }
  }
}
